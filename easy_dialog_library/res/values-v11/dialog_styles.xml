<resources>

    <style name="Theme.Dialog" parent="android:style/Theme.Holo.Dialog">
        <item name="android:windowBackground">@drawable/dialog_full_holo_dark</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="dialogTitleText">@style/DialogTitleText</item>
        <item name="dialogSubtitleText">@style/DialogSubtitle</item>
        <item name="dialogIcon">@style/DialogIcon</item>
        <item name="dialogTitleProgress">@style/DialogTitleProgress</item>
        <item name="dialogTitleDivider">@style/DialogTitleDivider</item>
        <item name="dialogMessageText">@style/DialogMessageText</item>
        <item name="dialogProgress">@style/DialogIndeterminateProgress</item>
        <item name="dialogProgressText">@style/DialogProgressMessage</item>
        <item name="dialogHorizontalProgress">@style/DialogHorizontalProgress</item>
        <item name="dialogHorizontalProgressText">@style/DialogHorizontalProgressText</item>
        <item name="dialogHorizontalProgressMessage">@style/DialogHorizontalProgressMessage</item>
        <item name="dialogWebView">@style/DialogWebView</item>
        <item name="dialogListView">@style/DialogListView</item>
        <item name="dialogListItemLayout">@layout/dialog_list_item</item>
        <item name="dialogListItemBackground">@drawable/gv_border_black</item>
        <item name="dialogListItemTextColor">#FFFFFFFF</item>
        <item name="dialogListItemCheckBoxDrawable">@drawable/btn_check_holo_dark</item>
        <item name="dialogListItemRadioButtonDrawable">@drawable/btn_radio_holo_dark</item>
        <item name="dialogGridView">@style/DialogGridView</item>
        <item name="dialogEditText">@style/DialogEditText</item>
        <item name="dialogCheckBox">@style/DialogCheckBox</item>
        <item name="dialogButtonDivider">@style/DialogButtonDivider</item>
        <item name="dialogButtonDividers">@style/DialogButtonDividers</item>
        <item name="dialogButton">@style/DialogBottomButton</item>
    </style>

    <style name="Theme.Dialog.Light" parent="android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@drawable/dialog_full_holo_light</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="dialogTitleText">@style/DialogTitleText.Light</item>
        <item name="dialogSubtitleText">@style/DialogSubtitle.Light</item>
        <item name="dialogIcon">@style/DialogIcon</item>
        <item name="dialogTitleProgress">@style/DialogTitleProgress</item>
        <item name="dialogTitleDivider">@style/DialogTitleDivider</item>
        <item name="dialogMessageText">@style/DialogMessageText.Light</item>
        <item name="dialogProgress">@style/DialogIndeterminateProgress</item>
        <item name="dialogProgressText">@style/DialogProgressMessage.Light</item>
        <item name="dialogHorizontalProgress">@style/DialogHorizontalProgress.Light</item>
        <item name="dialogHorizontalProgressText">@style/DialogHorizontalProgressText.Light</item>
        <item name="dialogHorizontalProgressMessage">@style/DialogHorizontalProgressMessage.Light</item>
        <item name="dialogWebView">@style/DialogWebView</item>
        <item name="dialogListView">@style/DialogListView.Light</item>
        <item name="dialogListItemLayout">@layout/dialog_list_item</item>
        <item name="dialogListItemBackground">@drawable/gv_border_light</item>
        <item name="dialogListItemTextColor">#FF040404</item>
        <item name="dialogListItemCheckBoxDrawable">@drawable/btn_check_holo_light</item>
        <item name="dialogListItemRadioButtonDrawable">@drawable/btn_radio_holo_light</item>
        <item name="dialogGridView">@style/DialogGridView.Light</item>
        <item name="dialogEditText">@style/DialogEditText.Light</item>
        <item name="dialogCheckBox">@style/DialogCheckBox.Light</item>
        <item name="dialogButtonDivider">@style/DialogButtonDivider.Light</item>
        <item name="dialogButtonDividers">@style/DialogButtonDividers.Light</item>
        <item name="dialogButton">@style/DialogBottomButton.Light</item>
    </style>

    <style name="Theme.Dialog.ICS">
        <item name="android:windowBackground">@drawable/dialog_bg_ics</item>
        <item name="dialogButton">@style/DialogBottomButton.Transparent</item>
    </style>

    <style name="Theme.Dialog.JB">
        <item name="android:windowBackground">@drawable/dialog_bg_ics</item>
    </style>

    <style name="Theme.Dialog.Light.ICS">
        <item name="dialogButton">@style/DialogBottomButton.Transparent.Light</item>
    </style>

</resources>